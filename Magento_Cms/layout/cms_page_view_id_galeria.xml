<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument name="title" xsi:type="string">Galeria</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="gallery.list" template="GardenLawn_Hyvatheme::gallery.phtml">
                <block class="Magento\Framework\View\Element\Template" name="gallery.head" template="Magento_Theme::html/head/pagination.phtml" />
            </block>
        </referenceContainer>
    </body>
</page>
