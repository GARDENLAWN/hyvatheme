<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <!-- Security Best Practices -->
        <policy id="base-uri">
            <values>
                <value id="base-uri-self" type="host">'self'</value>
            </values>
        </policy>

        <policy id="default-src">
            <values>
                <value id="default-src-self" type="host">'self'</value>
                <value id="default-src-s3" type="host">*.amazonaws.com</value>
                <value id="default-src-cloudfront" type="host">*.cloudfront.net</value>

                <!-- GardenLawn -->
                <value id="default-src-gardenlawn-wildcard" type="host">*.gardenlawn.pl</value>
                <value id="default-src-gardenlawn-root" type="host">gardenlawn.pl</value>

                <!-- AmRobots -->
                <value id="default-src-amrobots-wildcard" type="host">*.am-robots.pl</value>
                <value id="default-src-amrobots-root" type="host">am-robots.pl</value>

                <!-- FinnPolska -->
                <value id="default-src-finnpolska-wildcard" type="host">*.finnpolska.pl</value>
                <value id="default-src-finnpolska-root" type="host">finnpolska.pl</value>
            </values>
        </policy>

        <policy id="frame-src">
            <values>
                <value id="frame-src-self" type="host">'self'</value>
                <value id="frame-src-s3" type="host">*.amazonaws.com</value>
                <value id="frame-src-cloudfront" type="host">*.cloudfront.net</value>

                <value id="frame-src-gardenlawn-wildcard" type="host">*.gardenlawn.pl</value>
                <value id="frame-src-gardenlawn-root" type="host">gardenlawn.pl</value>

                <value id="frame-src-amrobots-wildcard" type="host">*.am-robots.pl</value>
                <value id="frame-src-amrobots-root" type="host">am-robots.pl</value>

                <value id="frame-src-finnpolska-wildcard" type="host">*.finnpolska.pl</value>
                <value id="frame-src-finnpolska-root" type="host">finnpolska.pl</value>

                <value id="frame-src-geowidget" type="host">*.inpost.pl</value>
                <value id="frame-src-youtube" type="host">*.youtube.com</value>
                <value id="frame-src-google" type="host">*.google.com</value>
            </values>
        </policy>

        <policy id="img-src">
            <values>
                <value id="img-src-self" type="host">'self'</value>
                <value id="img-src-s3" type="host">*.amazonaws.com</value>
                <value id="img-src-cloudfront" type="host">*.cloudfront.net</value>

                <value id="img-src-gardenlawn-wildcard" type="host">*.gardenlawn.pl</value>
                <value id="img-src-gardenlawn-root" type="host">gardenlawn.pl</value>

                <value id="img-src-amrobots-wildcard" type="host">*.am-robots.pl</value>
                <value id="img-src-amrobots-root" type="host">am-robots.pl</value>

                <value id="img-src-finnpolska-wildcard" type="host">*.finnpolska.pl</value>
                <value id="img-src-finnpolska-root" type="host">finnpolska.pl</value>

                <value id="img-src-tpay" type="host">*.tpay.com</value>
                <value id="img-src-paypal" type="host">*.paypal.com</value>
                <value id="img-src-paypalobjects" type="host">*.paypalobjects.com</value>
                <value id="img-src-unsplash-wildcard" type="host">*.unsplash.com</value>
                <value id="img-src-unsplash-root" type="host">unsplash.com</value>
                <value id="img-src-inpost" type="host">*.inpost.pl</value>
                <value id="img-src-paynow" type="host">*.paynow.pl</value>
                <value id="img-src-youtube" type="host">*.youtube.com</value>
                <value id="img-src-google" type="host">*.google.com</value>
                <value id="img-src-trawnikproducent" type="host">trawnikproducent.pl</value>
            </values>
        </policy>

        <policy id="style-src">
            <values>
                <value id="style-src-self" type="host">'self'</value>
                <value id="style-src-s3" type="host">*.amazonaws.com</value>

                <value id="style-src-gardenlawn-wildcard" type="host">*.gardenlawn.pl</value>
                <value id="style-src-gardenlawn-root" type="host">gardenlawn.pl</value>

                <value id="style-src-amrobots-wildcard" type="host">*.am-robots.pl</value>
                <value id="style-src-amrobots-root" type="host">am-robots.pl</value>

                <value id="style-src-finnpolska-wildcard" type="host">*.finnpolska.pl</value>
                <value id="style-src-finnpolska-root" type="host">finnpolska.pl</value>

                <value id="style-src-easypack24" type="host">*.easypack24.net</value>
                <value id="style-src-inpost" type="host">*.inpost.pl</value>
                <value id="style-src-googleapis" type="host">https://fonts.googleapis.com</value>
            </values>
        </policy>

        <policy id="form-action">
            <values>
                <value id="form-action-eraty" type="host">https://wniosek.eraty.pl/formularz</value>
            </values>
        </policy>

        <policy id="script-src">
            <values>
                <value id="script-src-self" type="host">'self'</value>
                <value id="script-src-unsafe" type="host">'unsafe-eval'</value>
                <value id="script-src-unsafe-hashes" type="host">'unsafe-hashes'</value>
                <value id="script-src-s3" type="host">*.amazonaws.com</value>

                <value id="script-src-gardenlawn-wildcard" type="host">*.gardenlawn.pl</value>
                <value id="script-src-gardenlawn-root" type="host">gardenlawn.pl</value>

                <value id="script-src-amrobots-wildcard" type="host">*.am-robots.pl</value>
                <value id="script-src-amrobots-root" type="host">am-robots.pl</value>

                <value id="script-src-finnpolska-wildcard" type="host">*.finnpolska.pl</value>
                <value id="script-src-finnpolska-root" type="host">finnpolska.pl</value>

                <value id="script-src-maps" type="host">*.googleapis.com</value>
                <value id="script-src-player" type="host">*.vimeo.com</value>
                <value id="script-src-newrelic" type="host">*.newrelic.com</value>
                <value id="script-src-nrdata" type="host">*.nr-data.net</value>
                <value id="script-src-paypal" type="host">*.paypal.com</value>
                <value id="script-src-paypalobjects" type="host">*.paypalobjects.com</value>
                <value id="script-src-inpost" type="host">*.inpost.pl</value>
                <value id="script-src-paynow" type="host">*.paynow.pl</value>
                <value id="script-src-youtube" type="host">*.youtube.com</value>
                <value id="script-src-google" type="host">*.google.com</value>
                <value id="script-src-google-analytics" type="host">www.google-analytics.com</value>
                <value id="script-src-googletagmanager" type="host">www.googletagmanager.com</value>
                <value id="script-src-googleapis" type="host">maps.googleapis.com</value>
                <value id="script-src-gstatic" type="host">www.gstatic.com</value>

