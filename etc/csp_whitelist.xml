<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="img-src">
            <values>
                <value id="gl-core-1" type="host">https://gardenlawn.s3.eu-central-1.amazonaws.com</value>
                <value id="data-scheme" type="hash">data</value>
                <value id="self-img" type="hash">self</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="s3-amazonaws" type="host">https://gardenlawn.s3.eu-central-1.amazonaws.com</value>
                <value id="self-style" type="hash">self</value>
                <value id="unsafe-inline-style" type="hash">unsafe-inline</value>
            </values>
        </policy>
        <policy id="style-src-elem">
            <values>
                <value id="gl-core-2" type="host">https://gardenlawn.s3.eu-central-1.amazonaws.com</value>
                <value id="gl-core-9" type="hash">self</value>
                <value id="gl-core-10" type="hash">unsafe-inline</value>
            </values>
        </policy>
        <policy id="script-src-elem">
            <values>
                <value id="gl-core-3" type="host">https://gardenlawn.s3.eu-central-1.amazonaws.com</value>
                <value id="unsafe-inline" type="hash">unsafe-inline</value>
                <value id="self" type="hash">self</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="gl-core-4" type="host">https://gardenlawn.s3.eu-central-1.amazonaws.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
