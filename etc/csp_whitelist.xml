        <policy id="connect-src">
            <values>
                <value id="connect-src-self" type="host">'self'</value>
                <value id="connect-src-s3" type="host">*.amazonaws.com</value>

                <value id="connect-src-gardenlawn-wildcard" type="host">*.gardenlawn.pl</value>
                <value id="connect-src-gardenlawn-root" type="host">gardenlawn.pl</value>

                <value id="connect-src-amrobots-wildcard" type="host">*.am-robots.pl</value>
                <value id="connect-src-amrobots-root" type="host">am-robots.pl</value>

                <value id="connect-src-finnpolska-wildcard" type="host">*.finnpolska.pl</value>
                <value id="connect-src-finnpolska-root" type="host">finnpolska.pl</value>

                <value id="connect-src-youtube" type="host">*.youtube.com</value>
                <value id="connect-src-google" type="host">*.google.com</value>
                <value id="connect-src-inpostpay" type="host">inpostpay-widget-api.inpost.pl</value>
                <value id="connect-src-google-analytics" type="host">www.google-analytics.com</value>
            </values>
        </policy>

        <policy id="font-src">
            <values>
                <value id="font-src-self" type="host">'self'</value>
                <value id="font-src-data" type="host">data:</value>
                <value id="font-src-s3" type="host">*.amazonaws.com</value>

                <value id="font-src-gardenlawn-wildcard" type="host">*.gardenlawn.pl</value>
                <value id="font-src-gardenlawn-root" type="host">gardenlawn.pl</value>

                <value id="font-src-amrobots-wildcard" type="host">*.am-robots.pl</value>
                <value id="font-src-amrobots-root" type="host">am-robots.pl</value>

                <value id="font-src-finnpolska-wildcard" type="host">*.finnpolska.pl</value>
                <value id="font-src-finnpolska-root" type="host">finnpolska.pl</value>

                <value id="font-src-geowidget" type="host">*.easypack24.net</value>
                <value id="font-src-googleapis" type="host">https://fonts.googleapis.com</value>
                <value id="font-src-gstatic" type="host">https://fonts.gstatic.com</value>
            </values>
